[ -e ~/.yelp_rc ] && { source ~/.yelp_rc; }
[ -e ~/.shared_rc ] && { source ~/.shared_rc; }

function names_only() { 
  sed 's/^[ \t]*//' | cut -f 1 -d ' ' | sed 's/.*//'
}

alias ls='ls --color=auto'
alias edit-profile='vim ~/.profile && source ~/.profile'

alias start-sandbox='cd ~/pg/yelp-main && sandbox -v --minimal'

